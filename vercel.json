{
    "version": 2,
    "builds": [
      {
        "src": "NUESTRO-PUEBLO/server/**", 
        "use": "@vercel/node"
      },
      {
        "src": "NUESTRO-PUEBLO/client/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "NUESTRO-PUEBLO/server/$1"
      },
      {
        "src": "/(.*)",
        "dest": "NUESTRO-PUEBLO/client/$1"
      }
    ]
  }
  
  